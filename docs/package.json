{
  "name": "docker-stockfish",
  "license": "AGPL-3.0-or-later",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cd .. && PORT=4000 python3 ./server/server.py",
    "dev:setup": "sudo ./stockfish/install.sh",
    "serve": "concurrently \"npm:serve:server\" \"npm:serve:client\"",
    "serve:client": "http-server -p 3000 ./client",
    "serve:server": "docker compose up"
  },
  "devDependencies": {
    "@ivangabriele/prettier-config": "3.1.0",
    "concurrently": "8.0.1",
    "http-server": "14.1.1"
  },
  "prettier": "@ivangabriele/prettier-config"
}
